@*用户积分排行榜-显示10条*@

@model IEnumerable<Web.Model.Entities.User>
@if(Model.Count() != 0)
{
    <ol>
        @foreach(var item in Model)
        {
            <li>
                <span class="top_score">@item.Score</span>
                <span class="user_name"><a href="@Url.Content($"/User/Index/{item.UserId}")">@item.Name</a></span>
            </li>
        }
    </ol>
}
else
{
    <div>无</div>
}